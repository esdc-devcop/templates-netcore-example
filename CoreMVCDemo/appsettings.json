{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "GoCWebTemplate": {
    "version": "v4_0_32", //change to current CDTS version
    "environment": "PROD_UNSECURE", //change to the appropriate environment for your application
    "gcToolsModal": true,
    "sessionTimeOut": {
      "enabled": true,
      "inactivity": 200000
    },
    "leavingSecureSiteWarning": {
      "enabled": true,
      "redirectURL": "https://www.google.ca",
      "displayModalWindow": false
    }
  }
}
